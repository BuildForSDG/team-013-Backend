{
  "name": "t-grade",
  "version": "0.0.1",
  "description": "T-Grade is a platform for grading qualified teachers and linking them to schools in thier community ",
  "keywords": [
    "Angular",
    "ExpressJS",
    "MongoDB",
    "Teacher Grading and Placement Platform",
    "Express"
  ],
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "eslint \"src/**/*.js\"",
    "start": "parcel ./src/index.html",
    "build": "rimraf dist/  && babel --presets es2015 -d dist/ src",
    "dev": "npm run build && nodemon dist/index.js"
  },
  "author": {
    "name": "Build For SDG",
    "email": "buildforsdg@andela.com"
  },
  "homepage": "https://github.com/BuildForSDG/js-starter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuildForSDG/js-starter.git"
  },
  "bugs": {
    "url": "https://github.com/BuildForSDG/js-starter/issues"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-cli": "^6.26.0",
    "babel-jest": "^25.4.0",
    "babel-preset-es2015": "^6.24.1",
    "cors": "^2.8.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
